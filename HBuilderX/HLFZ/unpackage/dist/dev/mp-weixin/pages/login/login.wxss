
.login-view {
	background-color: red;
}
.login-button {
	position: absolute;
	width: 50%;
	left: 25%;
	top: 60%;
}
.login-image {
	box-shadow: 5px 5px rgba(100, 255, 0, .6);
	width: 100%;
	height: 100%;
	background-color: #808080;
	border-radius: 10rpx;
	overflow: hidden;
}
page {
	text-align: center;
	cursor: default;
	-webkit-user-select: none;
	user-select: none;
	width: 100%;
	overflow-x: hidden;
	background-color: rgba(247, 56, 9, .5);
}

