package com.hz.love.service;

import org.springframework.web.multipart.MultipartFile;

import com.hz.love.model.CCmFile;
import com.hz.love.result.ResultEntity;

/**
 * <b>概述</b>：<br>
 * --文件上传下载--
 * <p>
 * <b>功能</b>：<br>
 * --文件上传下载--
 *
 * @author HZ
 */
public interface IFileService {

    /**
     * @uploadFile:文件上传
     * @param file 文件
     * @param fileInfo 文件信息
     * @return
     * @return ResultEntity
     * @date 2020年1月10日 下午4:55:04
     * @author HZ
     */
    ResultEntity uploadFile(MultipartFile file, CCmFile fileInfo);

}
